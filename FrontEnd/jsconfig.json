{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
      "@pages/*": ["./pages/*"],
      "@components/*": ["./components/*"],
      "@services/*": ["./services/*"],
      "@context/*": ["./context/*"],
      "@hooks/*": ["./hooks/*"],
      "@utils/*": ["./utils/*"],
      "@styles/*": ["./styles/*"],
      "@assets/*": ["./assets/*"],
      "@redux/*": ["./redux/*"]
    },
    "jsx": "react",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": ["node_modules", "build", "dist"]
} 